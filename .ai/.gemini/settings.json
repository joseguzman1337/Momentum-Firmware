{
  "tools": {
    "autoAccept": false,
    "exclude": [
      "run_shell_command(rm -rf)",
      "run_shell_command(sudo)"
    ],
    "sandbox": false
  },
  "model": {
    "compressionThreshold": 0.6
  },
  "general": {
    "checkpointing": {
      "enabled": true
    },
    "vimMode": false
  },
  "context": {
    "fileFiltering": {
      "respectGitIgnore": true
    }
  },
  "ui": {
    "theme": "GitHub"
  },
  "privacy": {
    "usageStatisticsEnabled": false
  }
}