{
    "theme": "GitHub",
    "autoAccept": false,
    "sandbox": false,
    "vimMode": false,
    "checkpointing": {
        "enabled": true
    },
    "fileFiltering": {
        "respectGitIgnore": true
    },
    "usageStatisticsEnabled": false,
    "chatCompression": {
        "contextPercentageThreshold": 0.6
    },
    "excludeTools": [
        "run_shell_command(rm -rf)",
        "run_shell_command(sudo)"
    ]
}