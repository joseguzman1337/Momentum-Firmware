        <line choice="installer_choice_5"/>
        <line choice="installer_choice_6"/>
    </choices-outline>
    <choice id="installer_choice_1" title="INSTALLER_CHOICE_1_TITLE" description="INSTALLER_CHOICE_1_DESCRIPTION">
        <pkg-ref id="com.alfa-network-inc.Realtek109Driver"/>
    </choice>
    <choice id="installer_choice_2" start_enabled="installer_choice_2_enabled()" enabled="installer_choice_2_enabled()" start_selected="installer_choice_2_selected(true)" selected="installer_choice_2_selected(false)" title="INSTALLER_CHOICE_2_TITLE" description="">
        <pkg-ref id="com.alfa-network-inc.RealtekUtility1014"/>
    </choice>
    <choice id="installer_choice_3" start_visible="false" title="INSTALLER_CHOICE_3_TITLE" description="INSTALLER_CHOICE_3_DESCRIPTION">
        <pkg-ref id="com.alfa-network-inc.RealtekSerialsInstaller"/>
    </choice>
    <choice id="installer_choice_4" start_visible="false" title="INSTALLER_CHOICE_4_TITLE" description="INSTALLER_CHOICE_4_DESCRIPTION">
        <pkg-ref id="com.alfa-network-inc.MediatekSerialsInstaller"/>
    </choice>
    <choice id="installer_choice_5" start_enabled="installer_choice_5_enabled()" enabled="installer_choice_5_enabled()" start_selected="installer_choice_5_selected(true)" selected="installer_choice_5_selected(false)" title="INSTALLER_CHOICE_5_TITLE" description="">
        <pkg-ref id="com.alfa-network-inc.Uninstall"/>
    </choice>
    <choice id="installer_choice_6" start_visible="false" start_enabled="installer_choice_6_enabled()" enabled="installer_choice_6_enabled()" start_selected="installer_choice_6_selected(true)" selected="installer_choice_6_selected(false)" title="Realtek Uninstall" description="">
        <pkg-ref id="com.alfa-network.inc.RealtekUninstall"/>
