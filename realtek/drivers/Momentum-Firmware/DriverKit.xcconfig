// DriverKit Extension Build Configuration
// This configuration should be applied to the DriverKit extension target

// Disable On-Demand Resources (not supported for DriverKit extensions)
ENABLE_ON_DEMAND_RESOURCES = NO

// Base SDK
SDKROOT = driverkit

// Deployment target
MACOSX_DEPLOYMENT_TARGET = 10.15

// Swift version
SWIFT_VERSION = 5.9

// Enable hardened runtime
ENABLE_HARDENED_RUNTIME = YES

// Product bundle identifier
PRODUCT_BUNDLE_IDENTIFIER = com.rtl8814au.driver

// Product name
PRODUCT_NAME = RTL8814AUDriver

// Skip install
SKIP_INSTALL = NO

// Strip symbols
STRIP_INSTALLED_PRODUCT = NO
COPY_PHASE_STRIP = NO

// Code signing
CODE_SIGN_STYLE = Automatic
ENABLE_HARDENED_RUNTIME = YES

// DriverKit specific settings
DRIVERKIT_DEPLOYMENT_TARGET = 19.0
