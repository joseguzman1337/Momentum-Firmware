<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>CFBundleDevelopmentRegion</key>
    <string>$(DEVELOPMENT_LANGUAGE)</string>
    <key>CFBundleExecutable</key>
    <string>$(EXECUTABLE_NAME)</string>
    <key>CFBundleIdentifier</key>
    <string>com.rtl8814au.driver</string>
    <key>CFBundleInfoDictionaryVersion</key>
    <string>6.0</string>
    <key>CFBundleName</key>
    <string>$(PRODUCT_NAME)</string>
    <key>CFBundlePackageType</key>
    <string>$(PRODUCT_BUNDLE_PACKAGE_TYPE)</string>
    <key>CFBundleShortVersionString</key>
    <string>1.0.0</string>
    <key>CFBundleVersion</key>
    <string>1</string>
    
    <!-- DriverKit Configuration -->
    <key>IOKitPersonalities</key>
    <dict>
        <key>RTL8814AUDriver</key>
        <dict>
            <key>CFBundleIdentifier</key>
            <string>com.rtl8814au.driver</string>
            <key>IOClass</key>
            <string>RTL8814AUDriver</string>
            <key>IOProviderClass</key>
            <string>IOUSBHostDevice</string>
            <key>IOUserClass</key>
            <string>RTL8814AUDriver</string>
            
            <!-- USB Device Matching -->
            <key>idVendor</key>
            <integer>3034</integer> <!-- 0x0bda = 3034 -->
            <key>idProduct</key>
            <array>
                <integer>34835</integer> <!-- 0x8813 = 34835 -->
                <integer>34836</integer> <!-- 0x8814 = 34836 -->
            </array>
            
            <!-- Device Properties -->
            <key>bConfigurationValue</key>
            <integer>1</integer>
            <key>bInterfaceNumber</key>
            <integer>0</integer>
        </dict>
    </dict>
    
    <!-- System Extension Configuration -->
    <key>NSSystemExtensionUsageDescription</key>
    <string>This system extension provides driver support for Realtek RTL8814AU USB WiFi adapters.</string>
    
    <!-- Required Entitlements -->
    <key>OSBundleUsageDescription</key>
    <string>RTL8814AU WiFi Driver for macOS</string>
    
    <!-- Minimum macOS Version -->
    <key>LSMinimumSystemVersion</key>
    <string>15.0</string>
</dict>
</plist>
